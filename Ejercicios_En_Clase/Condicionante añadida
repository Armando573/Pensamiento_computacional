IF dentro de un IF

Algoritmo sin_titulo
	num <- 0
	Escribir "Ingresa tu número"
	Leer num
	
	Si num == 0 Entonces
		
		Escribir "El 0 es un número neutro"
		
	SiNo
		
	
	
		si num > 0 Entonces
			
			Escribir "El numero es positivo"
		SiNo
		
			
			Escribir  "El numero es negativo"
		
		
		
	FinSi
FinSi
	
FinAlgoritmo
